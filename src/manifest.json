{
  "manifest_version": 2,

  "name": "Go home!",
  "description": "This extension shows the next bus passing times",
  "version": "0.1",

  "browser_action": {
    "default_icon": "bus.png", // Icon made by https://www.flaticon.com/authors/freepik from www.flaticon.com
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://plan-interactif.tcl.fr/"
  ]
}
